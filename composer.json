{
    "name": "juniora/laravel-nova-search",
    "description": "A package that extends search for laravel nova",
    "keywords": [
        "akki-io",
        "laravel-nova-search"
    ],
    "homepage": "https://github.com/akki-io/laravel-nova-search",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Akki Khare",
            "email": "hello@akki.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AkkiIo\\LaravelNovaSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AkkiIo\\LaravelNovaSearch\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
